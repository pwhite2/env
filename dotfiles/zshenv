# RCM aliases
alias dotfiles='cd ~/src/env/dotfiles'
alias lsrc='lsrc -d ~/src/env/dotfiles'
alias mkrc='mkrc -d ~/src/env/dotfiles'
alias rcdn='rcdn -d ~/src/env/dotfiles'
alias rcup='rcup -d ~/src/env/dotfiles'

# Source aliases generated by config.sh
source ~/.aliases
